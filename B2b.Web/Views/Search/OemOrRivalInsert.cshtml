@using System.Web
@using B2b.Web.v4.Models.Helper

<div class="tab-container clearfix">
    <ul class="nav-tabs clearfix">

        <li class="active"><a href="#oemInsert_@ViewBag.ProductId" data-toggle="tab">Oem</a></li>
        <li class=""><a href="#rivalcodeInsert_@ViewBag.ProductId" data-toggle="tab">Rakip</a></li>

    </ul>

    <div class="tab-content clearfix">
        <div class="tab-pane active" id="oemInsert_@ViewBag.ProductId">
            <div class="row">
                <div class="col-md-9">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="input-group input-criteria">
                                <span class="input-group-addon no-minwidth-zero">Araç Marka</span>
                                <input type="text" id="oemBrandName" class="form-control" placeholder="Araç Marka Giriniz." />
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="input-group input-criteria">
                                <span class="input-group-addon no-minwidth-zero">OEM No</span>
                                <input type="text" id="oemNo" class="form-control" required placeholder="Oem No Giriniz." />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <a href="javascript:;" class="btn btn-md btn-block btn-success" onclick="saveOemOrRival(0);"><i class="fa fa-check fa-lg"></i> Kaydet</a>
                </div>
            </div>
        </div><!-- End .tab-pane -->

        <div class="tab-pane" id="rivalcodeInsert_@ViewBag.ProductId">
            <div class="row">
                <div class="col-md-9">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="input-group input-criteria">
                                <span class="input-group-addon no-minwidth-zero">Araç Marka</span>
                                <input type="text" id="rivalBrandName" class="form-control" placeholder="Araç Marka Giriniz." />
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="input-group input-criteria">
                                <span class="input-group-addon no-minwidth-zero">Rakip Kod</span>
                                <input type="text" id="rivalNo" class="form-control" required placeholder="Rakip Kod Giriniz." />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <a href="javascript:;" class="btn btn-md btn-block btn-success" onclick="saveOemOrRival(1);"><i class="fa fa-check fa-lg"></i> Kaydet</a>
                </div>
            </div>
        </div><!-- End .tab-pane -->
    </div><!-- End .tab-content -->
</div>
<script src="@Url.ContentVersioned("/Scripts/Pages/oemOrRivalInsert.js", HttpContext.Current.Request)"></script>
<script type="text/javascript">
    var productId;

    $(document).ready(function () {
        productId =   @ViewBag.ProductId ;


    });
</script>

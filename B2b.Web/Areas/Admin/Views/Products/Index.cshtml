                                                                    <input id="iProductManufacturerCode" type="text" ng-disabled="lockUpdate" ng-model="selectedProduct.ManufacturerCode" onkeypress="return fireMaxLenghtControl(this)" data-max-lenght="50" placeholder="Lütfen, Üretici Kodu Giriniz..." class="form-control" />
                                                                </div>
                                                            </div>


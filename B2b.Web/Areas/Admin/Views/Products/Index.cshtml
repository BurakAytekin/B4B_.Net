<div ng-controller="IndexController">
    <div class="page page-products">
        <div class="pageheader">
            <div class="page-bar">
                <ul class="page-breadcrumb">
                    <li>
                        <a href="/Admin"><i class="fa fa-home"></i> Eryaz</a>
                    </li>
                    <li>
                        <a href="/Admin/Products">Ürünler</a>
                    </li>
                </ul>
            </div>
        </div>

        <!-- page content -->
        <div class="pagecontent">
            <div class="row">
                <!-- col -->
                <div class="col-sm-12">
                    <!-- tile -->
                    <section class="tile">
                        <!-- tile header -->
                        <div class="tile-header bg-blue dvd dvd-btm">
                            <h1 class="custom-font"><strong>Ürün </strong>Ara</h1>
                        </div>
                        <!-- /tile header -->
                        <!-- tile widget -->
                        <div class="tile-widget">
                            <div class="form-horizontal">
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="iProductCode" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Kod</label>
                                            <div class="col-xs-12 col-sm-12 col-md-8">
                                                <div class="input-group">
                                                    <input id="iProductCode" type="text" ng-keypress="keypressEventSearch($event,searchCriteria,1)" ng-model="searchCriteria.Code" onkeypress="return fireMaxLenghtControl(this)" data-max-lenght="50" placeholder="Lütfen, Ürün Kodu Giriniz..." class="form-control" />
                                                    <span class="input-group-btn">
                                                        <button ng-click="ProductSelectOpen(searchCriteria);" class="btn btn-block btn-primary btn-rounded">
                                                            <i class="fa fa-search"></i>
                                                        </button>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label for="iProductName" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Ürün Adı</label>
                                            <div class="col-xs-12 col-sm-12 col-md-8">
                                                <div class="input-group">
                                                    <input id="iProductName" type="text" ng-model="searchCriteria.Name" ng-keypress="keypressEventSearch($event,searchCriteria,2)" onkeypress="return fireMaxLenghtControl(this)" data-max-lenght="255" placeholder="Lütfen, Ürün Adı Giriniz..." class="form-control" />
                                                    <span class="input-group-btn">
                                                        <button ng-click="ProductSelectOpen(searchCriteria);" class="btn btn-block btn-primary btn-rounded">
                                                            <i class="fa fa-search"></i>
                                                        </button>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-2">
                                        <div class="form-group">
                                            <div class="col-xs-12">
                                                <button ng-click="clear();" class="btn btn-block btn-danger btn-rounded">
                                                    <span class="hidden-sm">Temizle</span> <i class="fa fa-trash-o visible-sm"></i>
                                                </button>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="col-xs-12">
                                                <button ng-click="ProductSelectOpen(searchCriteria);" class="btn btn-block btn-primary btn-rounded">
                                                    <span class="hidden-sm">Gelişmiş Arama</span> <i class="fa fa-search visible-sm"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-4">
                                        <dl class="dl-horizontal">
                                            <dt class="text-orange">Ürün No:</dt>
                                            <dd> {{selectedProduct.Id}}</dd>
                                            <dt class="text-orange">Entegre No:</dt>
                                            <dd>{{selectedProduct.EntegreId}}</dd>
                                        </dl>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- /tile widget -->

                        <hr class="m-0" />

                        <!-- tile body -->
                        <div class="tile-body p-10">
                            <div class="row">
                                <div class="col-sm-12">
                                    <!-- #region Nav Tabs -->
                                    <ul role="tablist" class="nav nav-tabs tabs-dark">
                                        @if (AuthoritySalesman.Where(x => x.Id == 46).Count() == 0)
                                        {
                                            <li role="presentation" class="active"><a href="#tProductGeneralInformation" aria-controls="tProductGeneralInformation" role="tab" data-toggle="tab"><span class="hidden-sm">Genel</span> <i class="fa fa-book visible-sm"></i></a></li>
                                        }
                                        @if (AuthoritySalesman.Where(x => x.Id == 47).Count() == 0)
                                        {

                                            <li role="presentation"><a href="#tProductPriceInformation" ng-click="getProductPrices();" aria-controls="tProductPriceInformation" role="tab" data-toggle="tab"><span class="hidden-sm">Fiyat Bilgileri</span> <i class="fa fa-money visible-sm"></i></a></li>
                                        }
                                        @if (AuthoritySalesman.Where(x => x.Id == 48).Count() == 0)
                                        {
                                            <li role="presentation"><a href="#tProductAdditionalInformation" aria-controls="tProductAdditionalInformation" role="tab" data-toggle="tab"><span class="hidden-sm">Ek Bilgiler</span> <i class="fa fa-info-circle visible-sm"></i></a></li>
                                        }
                                        @if (AuthoritySalesman.Where(x => x.Id == 49).Count() == 0)
                                        {

                                            <li role="presentation"><a href="#tProductStockMovements" aria-controls="tProductStockMovements" role="tab" data-toggle="tab"><span class="hidden-sm">Stok Har.</span> <i class="fa fa-calendar visible-sm"></i></a></li>
                                        }
                                        @if (AuthoritySalesman.Where(x => x.Id == 50).Count() == 0)
                                        {
                                            <li role="presentation"><a href="#tProductEquivalentProducts" ng-click="getAlternativeProducts();" aria-controls="tProductEquivalentProducts" role="tab" data-toggle="tab"><span class="hidden-sm">Eşdeğer Ürünler</span> <i class="fa fa-cubes visible-sm"></i></a></li>
                                        }
                                        @if (AuthoritySalesman.Where(x => x.Id == 51).Count() == 0)
                                        {
                                            <li role="presentation"><a href="#tProductRelatedProducts" ng-click="getLinkedProducts();" aria-controls="tProductRelatedProducts" role="tab" data-toggle="tab"><span class="hidden-sm">İlgili Ürünler</span> <i class="fa fa-files-o visible-sm"></i></a></li>
                                        }
                                        @*@if (AuthoritySalesman.Where(x => x.Id == 52).Count() == 0)
                                        {
                                            <li role="presentation"><a href="#tProductReferenceNumbers" aria-controls="tProductReferenceNumbers" role="tab" data-toggle="tab"><span class="hidden-sm">Referans Numaraları</span> <i class="fa fa-copyright visible-sm"></i></a></li>
                                        }*@
                                        @*@if (AuthoritySalesman.Where(x => x.Id == 53).Count() == 0)
                                        {
                                            <li role="presentation"><a href="#tProductVehicleInformation" ng-click="getVehicleBrandModel();" aria-controls="tProductVehicleInformation" role="tab" data-toggle="tab"><span class="hidden-sm">Araç Bilgileri</span> <i class="fa fa-truck visible-sm"></i></a></li>
                                        }*@
                                        @if (AuthoritySalesman.Where(x => x.Id == 54).Count() == 0)
                                        {
                                            <li role="presentation"><a href="#tProductImages" ng-click="getPictureActive();" aria-controls="tProductImages" role="tab" data-toggle="tab"><span class="hidden-sm">Görseller</span> <i class="fa fa-image visible-sm"></i></a></li>
                                        }
                                        @if (AuthoritySalesman.Where(x => x.Id == 55).Count() == 0)
                                        {
                                            <li role="presentation"><a href="#tProductDescription" aria-controls="tProductDescription" role="tab" data-toggle="tab"><span class="hidden-sm">Açıklama</span> <i class="fa fa-file-text-o visible-sm"></i></a></li>
                                        }
                                    </ul>
                                    <!-- #endregion -->
                                    <!-- #region Tab Panes -->

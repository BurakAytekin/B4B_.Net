        //});

        function fireScrollOnHover() {
            $(".scroll-on-hover").mouseover(function () {
                $(this).removeClass("ellipsis");
                var maxscroll = $(this).width();
                var speed = maxscroll * 10;
                $(this).animate({
                    scrollLeft: maxscroll
                }, speed, "linear");
            });

            $(".scroll-on-hover").mouseout(function () {
                $(this).stop();
                $(this).addClass("ellipsis");
                $(this).animate({
                    scrollLeft: 0
                }, 'slow');
            });
        }

        $(window).load(function () {
            //firelModalProductSearchLoad();

            $('#select-all').change(function () {
                if ($(this).is(":checked")) {
                    $('#usersList .selectMe').prop('checked', true);
                }
                else {
                    $('#usersList .selectMe').prop('checked', false);
                }
            });
        });
    </script>

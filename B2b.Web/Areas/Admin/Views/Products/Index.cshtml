                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <div class="input-group input-group-sm">
                                                                        <input id="iProductGroup2" type="text" ng-disabled="lockUpdate" ng-model="selectedProduct.ProductGroup2" placeholder="Lütfen, Grup 2 Giriniz..." class="form-control" />
                                                                        <span class="input-group-btn">
                                                                            <button ng-disabled="lockUpdate" ng-click="productGroupsOpen();" class="btn btn-block btn-primary btn-rounded">
                                                                                <i class="fa fa-search"></i>
                                                                            </button>
                                                                        </span>
                                                                    </div>
                                                                </div>

@using System.Web
@using B2b.Web.v4.Models.Helper

@using B2b.Web.v4.Models.EntityLayer
@{ List<AuthorityGroup> AuthoritySalesman = (List<AuthorityGroup>)HttpContext.Current.Session["AuthoritySalesman"]; }
@{
    ViewBag.Title = "Müşteriler";
}

@section cStyleLibrary{}

@section cStyleCode{
    <style type="text/css">
        .ws-nowrap {
            max-width: 100px;
            white-space: nowrap;
        }

        .overflow-hidden {
            overflow: hidden;
        }

        .ellipsis {
            -ms-text-overflow: ellipsis;
            -o-text-overflow: ellipsis;
            text-overflow: ellipsis;
        }

        .alternativeProdcut {
            background-color: #C9302C !important;
            color: white !important;
        }

        #trigger-upload {
            color: white;
            background-color: #00ABC7;
            font-size: 14px;
            padding: 7px 20px;
            background-image: none;
        }

        #fine-uploader-manual-trigger .qq-upload-button {
            margin-right: 15px;
        }

        #fine-uploader-manual-trigger .buttons {
            width: 36%;
        }

        #fine-uploader-manual-trigger .qq-uploader .qq-total-progress-bar-container {
            width: 60%;
        }
    </style>
}

<div ng-controller="IndexController">
    <div class="page page-customers">
        <div class="pageheader">
            <div class="page-bar">
                <ul class="page-breadcrumb">
                    <li>
                        <a href="/Admin"><i class="fa fa-home"></i> Eryaz</a>
                    </li>
                    <li>
                        <a href="/Admin/Customers">Müşteriler</a>
                    </li>
                </ul>
            </div>
        </div>

        <!-- page content -->
        <div class="pagecontent">
            <div class="row">
                <!-- col -->
                <div class="col-sm-12">
                    <!-- tile -->
                    <section class="tile">
                        <!-- tile header -->
                        <div class="tile-header bg-blue dvd dvd-btm">
                            <h1 class="custom-font"><strong>Müşteri </strong>Ara</h1>
                        </div>
                        <!-- /tile header -->
                        <!-- tile widget -->
                        <div class="tile-widget">
                            <div class="form-horizontal">
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="iProductCode" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Kod</label>
                                            <div class="col-xs-12 col-sm-12 col-md-8">
                                                <div class="input-group">
                                                    <input id="iProductCode" type="text" ng-keypress="keypressEventSearch($event,searchCriteria,'selectedCustomer',1)" ng-model="searchCriteria.Code" placeholder="Lütfen, Müşteri Kodu Giriniz..." class="form-control" onkeypress="return fireMaxLenghtControl(this)" data-max-lenght="50" />
                                                    <span class="input-group-btn">
                                                        <button ng-click="CustomerSelectOpen(searchCriteria,'selectedCustomer');" class="btn btn-block btn-primary btn-rounded">
                                                            <i class="fa fa-search"></i>
                                                        </button>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label for="iProductName" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Müşteri Adı</label>
                                            <div class="col-xs-12 col-sm-12 col-md-8">
                                                <div class="input-group">
                                                    <input id="iProductName" type="text" ng-model="searchCriteria.Name" ng-keypress="keypressEventSearch($event,searchCriteria,'selectedCustomer',2)" placeholder="Lütfen, Müşteri Adı Giriniz..." class="form-control" onkeypress="return fireMaxLenghtControl(this)" data-max-lenght="255" />
                                                    <span class="input-group-btn">
                                                        <button ng-click="CustomerSelectOpen(searchCriteria,'selectedCustomer');" class="btn btn-block btn-primary btn-rounded">
                                                            <i class="fa fa-search"></i>
                                                        </button>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-2">
                                        <div class="form-group">
                                            <div class="col-xs-12">
                                                <button ng-click="clear();" class="btn btn-block btn-danger btn-rounded">
                                                    <span class="hidden-sm">Temizle</span> <i class="fa fa-trash-o visible-sm"></i>
                                                </button>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="col-xs-12">
                                                <button ng-click="CustomerSelectOpen(searchCriteria,'selectedCustomer');" class="btn btn-block btn-primary btn-rounded">
                                                    <span class="hidden-sm">Gelişmiş Arama</span> <i class="fa fa-search visible-sm"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-4">
                                        <dl class="dl-horizontal">
                                            <dt class="text-orange">Müşteri No:</dt>
                                            <dd> {{selectedCustomer.Id}}</dd>
                                            <dt class="text-orange">Entegre No:</dt>
                                            <dd>{{selectedCustomer.EntegreId}}</dd>
                                        </dl>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- /tile widget -->

                        <hr class="m-0" />

                        <!-- tile body -->
                        <div class="tile-body p-10">
                            <div class="row">
                                <div class="col-sm-12">
                                    <!-- #region Nav Tabs -->
                                    <ul id="customerDetailTabs" role="tablist" class="nav nav-tabs tabs-dark">
                                        @if (AuthoritySalesman.Where(x => x.Id == 56).Count() == 0)
                                        {
                                            <li role="presentation" class="active">
                                                <a id="link-customerDetail-generalTab" href="#tCustomerGeneralInformation" aria-controls="tCustomerGeneralInformation" role="tab" data-toggle="tab">
                                                    <span class="hidden-sm">Genel</span>
                                                    <i class="fa fa-book visible-sm"></i>
                                                </a>
                                            </li>
                                        }
                                        @if (AuthoritySalesman.Where(x => x.Id == 57).Count() == 0)
                                        {
                                            <li role="presentation">
                                                <a href="#tCustomerAdditionalInformation" aria-controls="tCustomerAdditionalInformation" role="tab" data-toggle="tab">
                                                    <span class="hidden-sm">Diğer Bilgiler</span>
                                                    <i class="fa fa-info-circle visible-sm"></i>
                                                </a>
                                            </li>
                                        }
                                        @if (AuthoritySalesman.Where(x => x.Id == 58).Count() == 0)
                                        {

                                            <li role="presentation">
                                                <a href="#tCustomerPassiveManufacturer" ng-click="getManufacturers();" aria-controls="tCustomerPassiveManufacturer" role="tab" data-toggle="tab">
                                                    <span class="hidden-sm">Pasif Üreticiler</span>
                                                    <i class="fa fa-calendar visible-sm"></i>
                                                </a>
                                            </li>
                                        }
                                        @if (AuthoritySalesman.Where(x => x.Id == 59).Count() == 0)
                                        {

                                            <li role="presentation">
                                                <a href="#tCustomerLicence" ng-click="getCustomerLicence();" aria-controls="tCustomerLicence" role="tab" data-toggle="tab">
                                                    <span class="hidden-sm">Lisans</span>
                                                    <i class="fa fa-cubes visible-sm"></i>
                                                </a>
                                            </li>
                                        }
                                        @if (AuthoritySalesman.Where(x => x.Id == 60).Count() == 0)
                                        {

                                            <li role="presentation">
                                                <a href="#tCustomerLogin" ng-click="getcustomerLoginLog();" aria-controls="tCustomerLogin" role="tab" data-toggle="tab">
                                                    <span class="hidden-sm">Login Logları</span> <i class="fa fa-files-o visible-sm"></i>
                                                </a>
                                            </li>
                                        }
                                        @if (AuthoritySalesman.Where(x => x.Id == 61).Count() == 0)
                                        {
                                            <li role="presentation">
                                                <a href="#tCustomerSearch" aria-controls="tCustomerSearch" role="tab" data-toggle="tab">
                                                    <span class="hidden-sm">Arama Kayıtları</span>
                                                    <i class="fa fa-copyright visible-sm"></i>
                                                </a>
                                            </li>
                                        }
                                        @if (AuthoritySalesman.Where(x => x.Id == 62).Count() == 0)
                                        {

                                            <li role="presentation">
                                                <a href="#tCustomerInstallmentCount" ng-click="getCustomerSpecialInstallment();" aria-controls="tCustomerInstallmentCount" role="tab" data-toggle="tab">
                                                    <span class="hidden-sm">Banka Taksit Sayısı</span> <i class="fa fa-truck visible-sm"></i>
                                                </a>
                                            </li>
                                        }
                                        @if (AuthoritySalesman.Where(x => x.Id == 63).Count() == 0)
                                        {

                                            <li role="presentation">
                                                <a href="#tCustomerAdditionalDiscount" aria-controls="tCustomerAdditionalDiscount" role="tab" data-toggle="tab">
                                                    <span class="hidden-sm">Ek İskonto</span>
                                                    <i class="fa fa-image visible-sm"></i>
                                                </a>
                                            </li>
                                        }
                                        @if (AuthoritySalesman.Where(x => x.Id == 64).Count() == 0)
                                        {

                                            <li role="presentation">
                                                <a id="btnUsers" href="#tCustomerDownstreamUser" ng-click="getCustomerUserList()" aria-controls="tCustomerDownstreamUser" role="tab" data-toggle="tab">
                                                    <span class="hidden-sm">Kullanıcılar</span>
                                                    <i class="fa fa-file-text-o visible-sm"></i>
                                                </a>
                                            </li>
                                        }
                                        @if (AuthoritySalesman.Where(x => x.Id == 65).Count() == 0)
                                        {

                                            <li role="presentation">
                                                <a href="#tCustomerAuthority" ng-click="getAuthorityCustomer()" aria-controls="tCustomerAuthority" role="tab" data-toggle="tab">
                                                    <span class="hidden-sm">Yetkilendirme</span>
                                                    <i class="fa fa-file-text-o visible-sm"></i>
                                                </a>
                                            </li>
                                        }

                                    </ul>
                                    <!-- #endregion -->
                                    <!-- #region Tab Panes -->
                                    <div class="tab-content">
                                        <div id="tCustomerGeneralInformation" role="tabpanel" class="tab-pane active">
                                            <div class="row form-horizontal form-group-sm">
                                                <div class="col-sm-4">
                                                    <div class="panel panel-default panel-filled">
                                                        <div class="panel-heading">
                                                            <h3 class="panel-title">Adres Bilgileri</h3>
                                                        </div>

                                                        <div class="panel-body p-10">
                                                            <div class="form-group mb-5">
                                                                <label for="iProductManufacturer" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Adres</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <input id="iProductManufacturer" type="text" ng-disabled="lockUpdate" ng-model="selectedCustomer.Address" placeholder="Lütfen, Adres Giriniz..." class="form-control" onkeypress="return fireMaxLenghtControl(this)" data-max-lenght="1000" />
                                                                </div>
                                                            </div>

                                                            <div class="form-group mb-5">
                                                                <label for="iProductManufacturerCode" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">İlçe</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <input id="iProductManufacturerCode" type="text" ng-disabled="lockUpdate" ng-model="selectedCustomer.Town" onkeypress="return fireMaxLenghtControl(this)" data-max-lenght="20" placeholder="Lütfen, İlçe Giriniz..." class="form-control" />
                                                                </div>
                                                            </div>

                                                            <div class="form-group mb-0">
                                                                <label for="iProductUnit" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">İl</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <input id="iProductUnit" type="text" ng-disabled="lockUpdate" onkeypress="return fireMaxLenghtControl(this)" data-max-lenght="20" ng-model="selectedCustomer.City" placeholder="Lütfen, İl Giriniz..." class="form-control" />
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="panel panel-default panel-filled">
                                                        <div class="panel-heading">
                                                            <h3 class="panel-title">Ticari Bilgiler</h3>
                                                        </div>

                                                        <div class="panel-body p-10">
                                                            <div class="form-group mb-5">
                                                                <label for="iProductGroup1" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Vergi Dairesi</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <input id="iProductGroup1" type="text" ng-disabled="lockUpdate" ng-model="selectedCustomer.TaxOffice" placeholder="Lütfen, Vergi Dairesi Giriniz..." class="form-control" onkeypress="return fireMaxLenghtControl(this)" data-max-lenght="30" />
                                                                </div>
                                                            </div>

                                                            <div class="form-group mb-0">
                                                                <label for="iProductGroup2" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Vergi No</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <input id="iProductGroup2" type="text" ng-disabled="lockUpdate" ng-model="selectedCustomer.TaxNumber" placeholder="Lütfen, Vergi No Giriniz..." class="form-control" onkeypress="return fireMaxLenghtControl(this)" data-max-lenght="20" />
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="col-sm-4">
                                                    <div class="panel panel-default panel-filled">
                                                        <div class="panel-heading">
                                                            <h3 class="panel-title">İletişim Bilgileri</h3>
                                                        </div>

                                                        <div class="panel-body p-10">
                                                            <div class="form-group mb-5">
                                                                <label for="iProductConditionCode" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Tel 1</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <input id="iProductConditionCode" type="text" ng-disabled="lockUpdate" ng-model="selectedCustomer.Tel1" placeholder="Lütfen, Tel 1 Giriniz..." class="form-control" onkeypress="return fireMaxLenghtControl(this)" data-max-lenght="50" />
                                                                </div>
                                                            </div>

                                                            <div class="form-group mb-5">
                                                                <label for="iProductConditionCode" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Tel 2</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <input id="iProductConditionCode" type="text" ng-disabled="lockUpdate" ng-model="selectedCustomer.Tel2" placeholder="Lütfen, Tel 2 Giriniz..." class="form-control" onkeypress="return fireMaxLenghtControl(this)" data-max-lenght="50" />
                                                                </div>
                                                            </div>

                                                            <div class="form-group mb-5">
                                                                <label for="iProductConditionCode" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Gsm 1</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <input id="iProductConditionCode" type="text" ng-disabled="lockUpdate" ng-model="selectedCustomer.Gsm1" placeholder="Lütfen, Gsm 1 Giriniz..." class="form-control" onkeypress="return fireMaxLenghtControl(this)" data-max-lenght="50" />
                                                                </div>
                                                            </div>

                                                            <div class="form-group mb-5">
                                                                <label for="iProductConditionCode" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Gsm 2</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <input id="iProductConditionCode" type="text" ng-disabled="lockUpdate" ng-model="selectedCustomer.Gsm2" placeholder="Lütfen, Gsm 2 Giriniz..." class="form-control" onkeypress="return fireMaxLenghtControl(this)" data-max-lenght="50" />
                                                                </div>
                                                            </div>

                                                            <div class="form-group mb-5">
                                                                <label for="iProductConditionCode" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Fax </label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <input id="iProductConditionCode" type="text" ng-disabled="lockUpdate" ng-model="selectedCustomer.Fax1" placeholder="Lütfen, Fax  Giriniz..." class="form-control" onkeypress="return fireMaxLenghtControl(this)" data-max-lenght="50" />
                                                                </div>
                                                            </div>

                                                            <div class="form-group mb-0">
                                                                <label for="iProductConditionCode" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">E-Posta</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <input id="iProductConditionCode" type="text" ng-disabled="lockUpdate" ng-model="selectedCustomer.Mail" placeholder="Lütfen, E-Posta Giriniz..." class="form-control" onkeypress="return fireMaxLenghtControl(this)" data-max-lenght="100" />
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="panel panel-default panel-filled">
                                                        <div class="panel-heading">
                                                            <h3 class="panel-title">Entegrasyon Bilgileri</h3>
                                                        </div>

                                                        <div class="panel-body p-10">
                                                            <div class="form-group mb-0">
                                                                <label for="iProductConditionCode" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">B2B Kodu</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <input id="iProductConditionCode" type="text" ng-disabled="lockUpdate" ng-model="selectedCustomer.B2bCode" placeholder="Lütfen, B2B Kodu  Giriniz..." class="form-control" onkeypress="return fireMaxLenghtControl(this)" data-max-lenght="50" />
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="col-sm-4">
                                                    <div class="panel panel-default panel-filled">
                                                        <div class="panel-heading">
                                                            <h3 class="panel-title">Depo Bilgileri</h3>
                                                        </div>

                                                        <div class="panel-body p-10" ng-repeat="warehouseItem in warehouseList">
                                                            <div class="form-group mb-0">
                                                                <label for="chckWarehouse{{warehouseItem.Id}}" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">{{warehouseItem.Name}}</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <label class="checkbox checkbox-custom-alt">
                                                                        <input type="checkbox" id="chckWarehouse{{warehouseItem.Id}}" ng-change="checkWarehouseUpdate(warehouseItem);" ng-model="warehouseItem.Checked" /><i></i>
                                                                    </label>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div id="tCustomerAdditionalInformation" role="tabpanel" class="tab-pane">
                                            <div class="row form-horizontal form-group-sm">
                                                <div class="col-sm-8">
                                                    <div class="panel panel-default panel-filled">
                                                        <div class="panel-heading">
                                                            <h3 class="panel-title">Ek Bilgiler</h3>
                                                        </div>

                                                        <div class="panel-body p-10">
                                                            <div class="form-group mb-5">
                                                                <label for="iProductConditionCode" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Temsilci</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <div class="input-group input-group-sm">
                                                                        <input id="iProductConditionCode" type="text" ng-disabled="lockUpdate" ng-model="selectedCustomer.Salesman.Name" placeholder="Lütfen, Temsilci Giriniz..." class="form-control" />
                                                                        <span class="input-group-btn">
                                                                            <button ng-disabled="lockUpdate" ng-click="salesmanSelectOpen(0,'selectedCustomer.Salesman');" class="btn btn-block btn-primary btn-rounded">
                                                                                <i class="fa fa-search"></i>
                                                                            </button>
                                                                        </span>
                                                                    </div>
                                                                </div>
                                                            </div>

                                                            <div class="form-group mb-5">
                                                                <label for="iProductConditionCode" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Satış Koşulu</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <div class="input-group input-group-sm">
                                                                        <input id="iProductConditionCode" type="text" ng-disabled="lockUpdate" ng-model="selectedCustomer.RuleCode" placeholder="Lütfen, Satış Koşulu Giriniz..." class="form-control" />
                                                                        <span class="input-group-btn">
                                                                            <button ng-disabled="lockUpdate" ng-click="customerRuleOpen(0);" class="btn btn-block btn-primary btn-rounded">
                                                                                <i class="fa fa-search"></i>
                                                                            </button>
                                                                        </span>
                                                                    </div>
                                                                </div>
                                                            </div>

                                                            <div class="form-group mb-5">
                                                                <label for="iProductOtherInformation1" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Kullanıcı Sayısı</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <select ng-model="selectedCustomer.NumberOfUser" style="width: 100%;" class="form-control" ng-options="item.Count as item.Count for item in userCountList"></select>
                                                                </div>
                                                            </div>

                                                            <div class="form-group mb-5">
                                                                <label for="iProductOtherInformation2" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Kullanıcı Sayısı Android</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <select ng-model="selectedCustomer.NumberOfUserAndroid" style="width: 100%;" class="form-control" ng-options="item.Count as item.Count for item in userCountList"></select>
                                                                </div>
                                                            </div>

                                                            <div class="form-group mb-5">
                                                                <label for="iProductOtherInformation3" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Kullanıcı Sayısı İos</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">
                                                                    <select ng-model="selectedCustomer.NumberOfUserIos" style="width: 100%;" class="form-control" ng-options="item.Count as item.Count for item in userCountList"></select>
                                                                </div>
                                                            </div>

                                                            <div class="form-group mb-0">
                                                                <label for="iProductOtherInformation5" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Cari Hareket Şifre</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-3">
                                                                    <div class="input-group input-group-sm">
                                                                        <label class="input-group-addon checkbox checkbox-custom-alt checkbox-custom-sm">
                                                                            <input type="checkbox" ng-click="" ng-model="selectedCustomer.IsCurrentAccountStatu" />&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <i></i>
                                                                        </label>
                                                                        <input id="iProductOtherInformation5" type="text" ng-disabled="lockUpdate" ng-model="selectedCustomer.CurrentAccountPassword" placeholder="Lütfen, Cari Hareket Şifre Giriniz..." class="form-control" />
                                                                    </div>
                                                                </div>
                                                                <div class="col-xs-12 col-sm-12 col-md-5">
                                                                    <input type="button" ng-disabled="lockUpdate" ng-click="showEmailSendScreenCustomer()" class="btn btn-warning btn-rounded btn-ef btn-ef-5 btn-ef-5a" value="Sıfırlama Gönder" />

                                                                </div>
                                                            </div>
                                                            <div class="form-group mb-0">
                                                                <label for="iProductOtherInformation5" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Sözleşme Onayı</label>
                                                                <div class="col-xs-12 col-sm-12 col-md-8">

@using System.Web
@using B2b.Web.v4.Models.Helper
@{
    ViewBag.Title = "Sipariş Detayı";
}

@section cStyleLibrary{}

@section cStyleCode{
    <style type="text/css">
        .table-responsive {
            overflow-x: auto;
        }
    </style>
}

    <div class="page page-order-detail">
        <div class="pageheader">
            <div class="page-bar">
                <ul class="page-breadcrumb">
                    <li>
                        <a href="/Admin"><i class="fa fa-home"></i> Eryaz</a>
                    </li>
                    <li>
                        <a href="/Admin/Orders/OrderDetail">Sipariş Detayı</a>
                    </li>
                </ul>
            </div>
        </div>

        <!-- #region Page Content -->
        <div class="pagecontent" ng-controller="OrderDetailController">
            <div class="row">
                <!-- col -->
                <div class="col-sm-12">
                    <!-- tile -->
                    <section class="tile">
                        <!-- tile header -->
                        <div class="tile-header bg-blue dvd dvd-btm">
                            <h1 class="custom-font"><strong>Sipariş </strong>Detayı</h1>
                            <ul class="controls">
                                <li role="presentation"><a href="#" ng-click="exportToExcel()" aria-expanded="true"><i class="fa fa-file-excel-o"></i> <strong>Excel'e Kaydet</strong></a></li>
                            </ul>
                        </div>
                        <!-- /tile header -->
                        <!-- tile widget -->
                        <div class="tile-widget">
                            <div class="form-horizontal">
                                <div class="row">
                                    <div class="col-sm-3">
                                        <div class="panel panel-default panel-filled">
                                            <div class="panel-heading">
                                                <h3 class="panel-title">Müşteri Bilgileri</h3>
                                            </div>

                                            <div class="panel-body p-10">
                                                <div class="form-group mb-5">
                                                    <label for="iOrderDetailCustomerCode" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Kodu</label>
                                                    <div class="col-xs-12 col-sm-12 col-md-8">
                                                        <input id="iOrderDetailCustomerCode" type="text" ng-model="orderHeader.Customer.Code" disabled="disabled" placeholder="-" class="form-control" />
                                                    </div>
                                                </div>

                                                <div class="form-group mb-5">
                                                    <label for="iOrderDetailCustomerName" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Ünvan</label>
                                                    <div class="col-xs-12 col-sm-12 col-md-8">
                                                        <textarea id="iOrderDetailCustomerName" disabled="disabled" row="6" cols="0" class="form-control">{{orderHeader.Customer.Name}}</textarea>
                                                    </div>
                                                </div>

                                                <div class="form-group mb-5">
                                                    <label for="iOrderDetailCustomerAddress" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Adres</label>
                                                    <div class="col-xs-12 col-sm-12 col-md-8">
                                                        <textarea id="iOrderDetailCustomerAddress" disabled="disabled" row="6" cols="0" class="form-control">{{orderHeader.Customer.AddressFull}}</textarea>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-6">

                                        <div class="panel panel-default panel-filled">
                                            <div class="panel-heading">
                                                <h3 class="panel-title">Ödeme Bilgileri</h3>
                                            </div>

                                            <div class="panel-body p-10">

                                                <table class="table table-striped table-hover table-custom">
                                                    <thead>
                                                        <tr>
                                                            <th class="table-title">Ödeme Türü</th>
                                                            <th class="table-title">Tutar</th>
                                                            <th class="table-title">Ödeme Tarihi</th>
                                                            <th class="table-title">Makbuz No</th>
                                                            <th class="table-title">Banka Adı</th>
                                                            <th class="table-title">Görsel</th>

                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr ng-repeat="OrderPaymentTypeItem in OrderPaymentTypeList">

                                                            <td class="item-price-col">{{OrderPaymentTypeItem.TypeStr}}</td>
                                                            <td class="item-price-col">{{OrderPaymentTypeItem.Total}}</td>
                                                            <td class="item-price-col">{{OrderPaymentTypeItem.PaymentDate | jsonDate | date:'dd.MM.yyyy'}}</td>
                                                            <td class="item-price-col">{{OrderPaymentTypeItem.ReceiptNo}}</td>
                                                            <td class="item-price-col">{{OrderPaymentTypeItem.BankName}}</td>
                                                            <td class="item-price-col"><a ng-if="OrderPaymentTypeItem.Image" ng-click="openImage(OrderPaymentTypeItem.Image)"><i class="fa fa-image"></i></a></td>

                                                        </tr>

                                                    </tbody>

                                                </table>
                                            </div>
                                        </div>

                                        <div class="panel panel-default panel-filled">
                                            <div class="panel-heading">
                                                <h3 class="panel-title">Bakiye Bilgileri</h3>
                                            </div>

                                            <div class="panel-body p-10">
                                                <div class="form-group mb-5">
                                                    <label for="iOrderDetailCustomerCode" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Alacak</label>
                                                    <div class="col-xs-12 col-sm-12 col-md-8">
                                                        <input id="iOrderDetailCustomerCode" type="text" ng-model="customerBalanceInfo.Debt" disabled="disabled" class="form-control" />
                                                    </div>
                                                </div>
                                                <div class="form-group mb-5">
                                                    <label for="iOrderDetailCustomerCode" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Borç</label>
                                                    <div class="col-xs-12 col-sm-12 col-md-8">
                                                        <input id="iOrderDetailCustomerCode" type="text" ng-model="customerBalanceInfo.Credit" disabled="disabled" class="form-control" />
                                                    </div>
                                                </div>

                                                <div class="form-group mb-5">
                                                    <label for="iOrderDetailCustomerCode" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Bakiye</label>
                                                    <div class="col-xs-12 col-sm-12 col-md-8">
                                                        <input id="iOrderDetailCustomerCode" type="text" ng-model="customerBalanceInfo.Balance" disabled="disabled" class="form-control" />
                                                    </div>
                                                </div>


                                                <table id="pDataTable" class="table table-striped table-hover table-custom shadowed">
                                                    <!---table-condensed table-striped table-hover-->
                                                    <thead>
                                                        <tr>
                                                            <th>Tarih</th>
                                                            <th>Vade Tarihi</th>
                                                            <th>Evrak No</th>
                                                            <th>İşlem Türü</th>
                                                            <th>Açıklama</th>
                                                            <th class="text-right">Alacak</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody class="context">
                                                        <tr ng-repeat="row in customerBalanceInfo.CheckAndContracts">
                                                            <td>{{row.Date | dateFilterErp  | date:'dd.MM.yyyy'}}</td>
                                                            <td>{{row.DueDate | dateFilterErp  | date:'dd.MM.yyyy'}}</td>
                                                            <td class="text-left">{{row.DocumentNo}}</td>
                                                            <td class="text-left">{{row.TransactionType}}</td>
                                                            <td class="text-left">{{row.Explanation}}</td>
                                                            <td class="text-right" ng-bind-html="row.CreditStr"></td>
                                                        </tr>
                                                    </tbody>
                                                </table>


                                            </div>
                                        </div>


                                    </div>

                                    <div class="col-sm-3">
                                        <div class="panel panel-default panel-filled">
                                            <div class="panel-heading">
                                                <h3 class="panel-title">Sipariş Bilgileri</h3>
                                            </div>

                                            <div class="panel-body p-10">
                                                <div class="form-group mb-5">
                                                    <label for="iOrderDetailNo" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Sip. No</label>
                                                    <div class="col-xs-12 col-sm-12 col-md-8 text-center">
                                                        <input id="iOrderDetailNo" type="text" ng-model="orderHeader.Id" disabled="disabled" placeholder="-" class="form-control" />
                                                    </div>
                                                </div>

                                                <div class="form-group mb-5">
                                                    <label for="iOrderDetailDate" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Sip. Tarih</label>
                                                    <div class="col-xs-12 col-sm-12 col-md-8 text-center">
                                                        <input id="iOrderDetailDate" type="text" value="{{orderHeader.CreateDate | dateFilter  | date:'dd.MM.yyyy  HH:mm'}}" disabled="disabled" placeholder="-" class="form-control" />
                                                    </div>
                                                </div>

                                                <div class="form-group mb-5">
                                                    <label for="iOrderDetailConfirmDate" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Onay Tarih</label>
                                                    <div class="col-xs-12 col-sm-12 col-md-8 text-center">
                                                        <input id="iOrderDetailConfirmDate" type="text" value="{{orderHeader.ConfirmDate | dateFilter  | date:'dd.MM.yyyy  HH:mm'}}" disabled="disabled" placeholder="-" class="form-control" />
                                                    </div>
                                                </div>

                                                <div class="form-group mb-5">
                                                    <label for="iOrderDetailStatu" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Durumu</label>
                                                    <div class="col-xs-12 col-sm-12 col-md-8 text-center">
                                                        <input id="iOrderDetailStatu" type="text" ng-model="orderHeader.StatusStr" disabled="disabled" placeholder="-" class="form-control" />
                                                    </div>
                                                </div>

                                                <div class="form-group mb-5">
                                                    <label for="iOrderDetailSender" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Gönderen</label>
                                                    <div class="col-xs-12 col-sm-12 col-md-8 text-left">
                                                        <textarea id="iOrderDetailSender" disabled="disabled" row="6" cols="0" class="form-control">{{orderHeader.SenderName}}</textarea>
                                                    </div>
                                                </div>

                                                <div class="form-group mb-0">
                                                    <label for="iOrderDetailConfirmation" class="col-xs-12 col-sm-5 col-md-4 control-label text-left hidden-xs hidden-sm">Onaylayan</label>
                                                    <div class="col-xs-12 col-sm-12 col-md-8 text-center">
                                                        <input id="iOrderDetailConfirmation" type="text" ng-model="orderHeader.ConfirmSalesmanName" disabled="disabled" placeholder="-" class="form-control" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- /tile widget -->

                        <hr class="m-0" />

                        <!-- tile body -->
                        <div class="tile-body p-10">
                            <div class="row">
                                <div class="col-sm-12 p-0">
                                    <div class="col-xs-12">
                                        <div class="brn-group pull-right">
                                            <button class="btn btn-default" ng-click="cancelChangesDetail()">
                                                <span class="glyphicon glyphicon-remove"></span>
                                            </button>

                                            <button class="btn btn-default" ng-click="ProductSelectOpen(searchCriteria);">
                                                <span class="glyphicon glyphicon-plus"></span>
                                            </button>

                                            @*<button class="btn btn-default" ng-click="addDetail()">
                                                <span class="glyphicon glyphicon-plus"></span>
                                            </button>*@
                                        </div>
                                    </div>

                                    <div class="p-5">&nbsp;</div>

                                    <div loading-container="tableOrderDetailParams.settings().$loading">
                                        <div class="col-xs-12">
                                            <div class="table-responsive">
                                                <table ng-table="tableOrderDetailParams" ng-form="tableOrderDetailForm" disable-filter="isAdding" demo-tracked-table="tableTracker" class="table table-condensed table-bordered table-striped">
                                                    <tr ng-repeat="row in $data" ng-form="rowForm" demo-tracked-table-row="row">
                                                        <td title="'Ürün Kodu'" filter="{ProductCode: 'text'}" sortable="'ProductCode'" ng-switch="row.isEditing" ng-class="ProductCode.$dirty ? 'bg-warning text-left' : 'text-left'" ng-form="ProductCode" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text">{{row.ProductCode}}</span>
                                                            <div class="controls" ng-class="ProductCode.$invalid && ProductCode.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="ProductCode" ng-model="row.ProductCode" class="typeahead editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>
                                                        <td title="'Adı '" filter="{ProductName: 'text'}" sortable="'ProductName'" ng-switch="row.isEditing" ng-class="ProductName.$dirty ? 'bg-warning text-left' : 'text-left'" ng-form="ProductName" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text">{{row.ProductName}}</span>
                                                            <div class="controls" ng-class="ProductName.$invalid && ProductName.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="ProductName" ng-model="row.ProductName" class="editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>

                                                        <td title="'Stok Adedi'" filter="{TotalQuantity: 'text'}" sortable="'TotalQuantity'" ng-switch="row.isEditing" ng-class="TotalQuantity.$dirty ? 'bg-warning text-left' : 'text-left'" ng-form="TotalQuantity" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text">{{row.TotalQuantity}}</span>
                                                            <div class="controls" ng-class="TotalQuantity.$invalid && ProductName.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="ProductName" ng-model="row.TotalQuantity" class="editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>

                                                        <td title="'Üretici '" filter="{Manufacturer: 'text'}" sortable="'Manufacturer'" ng-switch="row.isEditing" ng-class="Manufacturer.$dirty ? 'bg-warning text-left' : 'text-left'" ng-form="Manufacturer" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text">{{row.Manufacturer}}</span>
                                                            <div class="controls" ng-class="Manufacturer.$invalid && Manufacturer.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="Manufacturer" ng-model="row.Manufacturer" class="editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>
                                                        <td title="'Mik '" filter="{Quantity: 'text'}" sortable="'Quantity'" ng-switch="row.isEditing" ng-class="Quantity.$dirty ? 'bg-warning text-center' : 'text-center'" ng-form="Quantity" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text">{{row.Quantity}}</span>
                                                            <div class="controls" ng-class="Quantity.$invalid && Quantity.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="Unit" ng-model="row.Quantity" class="editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>
                                                        <td title="'Sevk Mik '" filter="{ShippedQty: 'text'}" sortable="'ShippedQty'" ng-switch="row.isEditing" ng-class="Quantity.$dirty ? 'bg-warning text-center' : 'text-center'" ng-form="ShippedQty" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text">{{row.ShippedQty}}</span>
                                                            <div class="controls" ng-class="Quantity.$invalid && Quantity.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="Unit" ng-model="row.ShippedQty" class="editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>
                                                        <td title="'Birim '" filter="{Unit: 'text'}" sortable="'Unit'" ng-switch="row.isEditing" ng-class="Unit.$dirty ? 'bg-warning text-center' : 'text-center'" ng-form="Unit" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text">{{row.Unit}}</span>
                                                            <div class="controls" ng-class="Unit.$invalid && Unit.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="Unit" ng-model="row.Unit" class="editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>
                                                        <td title="'Fiyat '" filter="{PriceStr: 'text'}" sortable="'PriceStr'" ng-switch="row.isEditing" ng-class="PriceStr.$dirty ? 'bg-warning text-right' : 'text-right'" ng-form="PriceStr" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text text-nowrap" ng-bind-html="row.PriceStr"></span>
                                                            <div class="controls" ng-class="PriceStr.$invalid && PriceStr.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="Unit" ng-model="row.PriceStr" class="editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>
                                                        <td title="'Isk1 '" filter="{Disc1: 'text'}" sortable="'Disc1'" ng-switch="row.isEditing" ng-class="Disc1.$dirty ? 'bg-warning text-center' : 'text-center'" ng-form="Disc1" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text">{{row.Disc1}}</span>
                                                            <div class="controls" ng-class="Disc1.$invalid && Disc1.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="Disc1" ng-model="row.Disc1" class="editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>
                                                        <td title="'Isk2 '" filter="{Disc2: 'text'}" sortable="'Disc2'" ng-switch="row.isEditing" ng-class="Disc2.$dirty ? 'bg-warning text-center' : 'text-center'" ng-form="Disc2" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text">{{row.Disc2}}</span>
                                                            <div class="controls" ng-class="Disc2.$invalid && Disc2.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="Disc2" ng-model="row.Disc2" class="editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>
                                                        @*<td title="'Isk3 '" filter="{Disc3: 'text'}" sortable="'Disc3'" ng-switch="row.isEditing" ng-class="Disc3.$dirty ? 'bg-warning text-center' : 'text-center'" ng-form="Disc3" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text">{{row.Disc3}}</span>
                                                            <div class="controls" ng-class="Disc3.$invalid && Disc3.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="Disc3" ng-model="row.Disc3" class="editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>
                                                        <td title="'Isk4 '" filter="{Disc4: 'text'}" sortable="'Disc4'" ng-switch="row.isEditing" ng-class="Disc4.$dirty ? 'bg-warning text-center' : 'text-center'" ng-form="Disc4" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text">{{row.Disc4}}</span>
                                                            <div class="controls" ng-class="Disc4.$invalid && Disc4.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="Disc4" ng-model="row.Disc4" class="editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>*@
                                                        <td title="'Cmp Isk '" filter="{DiscCampaign: 'text'}" sortable="'DiscCampaign'" ng-switch="row.isEditing" ng-class="DiscCampaign.$dirty ? 'bg-warning text-center' : 'text-center'" ng-form="DiscCampaign" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text">{{row.DiscCampaign}}</span>
                                                            <div class="controls" ng-class="DiscCampaign.$invalid && DiscCampaign.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="DiscCampaign" ng-model="row.DiscCampaign" class="editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>
                                                        <td title="'Cep Isk '" filter="{DiscSpecial: 'text'}" sortable="'DiscSpecial'" ng-switch="row.isEditing" ng-class="DiscSpecial.$dirty ? 'bg-warning text-center' : 'text-center'" ng-form="DiscSpecial" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text">{{row.DiscSpecial}}</span>
                                                            <div class="controls" ng-class="DiscSpecial.$invalid && DiscSpecial.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="DiscSpecial" ng-model="row.DiscSpecial" class="editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>
                                                        <td title="'Kupon Isk '" filter="{DiscCoupon: 'text'}" sortable="'DiscCoupon'" ng-switch="row.isEditing" ng-class="DiscCoupon.$dirty ? 'bg-warning text-center' : 'text-center'" ng-form="DiscCoupon" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text">{{row.DiscCoupon}}</span>
                                                            <div class="controls" ng-class="DiscCoupon.$invalid && DiscSpecial.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="DiscCoupon" ng-model="row.DiscCoupon" class="editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>
                                                        <td title="'Tutar'" filter="{AmountStr: 'text'}" sortable="'AmountStr'" ng-switch="row.isEditing" ng-class="AmountStr.$dirty ? 'bg-warning text-right' : 'text-right'" ng-form="AmountStr" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text text-nowrap" ng-bind-html="row.AmountStr"></span>
                                                            <div class="controls" ng-class="AmountStr.$invalid && AmountStr.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="AmountStr" ng-model="row.AmountStr" class="editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>
                                                        <td title="'Net Fiyat'" filter="{NetPriceStr: 'text'}" sortable="'NetPriceStr'" ng-switch="row.isEditing" ng-class="NetPriceStr.$dirty ? 'bg-warning text-right' : 'text-right'" ng-form="NetPriceStr" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text text-nowrap" ng-bind-html="row.NetPriceStr"></span>
                                                            <div class="controls" ng-class="NetPriceStr.$invalid && NetPriceStr.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="NetPriceStr" ng-model="row.NetPriceStr" class="editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>
                                                        <td title="'Net Tutar'" filter="{NetAmountStr: 'text'}" sortable="'NetAmountStr'" ng-switch="row.isEditing" ng-class="NetAmountStr.$dirty ? 'bg-warning text-right' : 'text-right'" ng-form="NetAmountStr" demo-tracked-table-cell>
                                                            <span ng-switch-default class="editable-text text-nowrap" ng-bind-html="row.NetAmountStr"></span>
                                                            <div class="controls" ng-class="NetAmountStr.$invalid && NetAmountStr.$dirty ? 'has-error' : ''" ng-switch-when="true">
                                                                <input type="text" name="NetAmountStr" ng-model="row.NetAmountStr" class="editable-input form-control input-sm" required ng-keypress="keypressOemEvent($event,row)" />
                                                            </div>
                                                        </td>
                                                        <td title="'Kdv'" filter="{VatAmountStr: 'text'}" sortable="'VatAmountStr'" ng-switch="row.isEditing" ng-class="VatAmountStr.$dirty ? 'bg-warning text-right' : 'text-right'" ng-form="VatAmountStr" demo-tracked-table-cell>
